{
    "disable_existing_loggers": false, 
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        }
    }, 
    "handlers": {
        "brew-view": {
            "class": "logging.StreamHandler", 
            "formatter": "simple", 
            "level": "INFO", 
            "stream": "ext://sys.stdout"
        }
    }, 
    "loggers": {
        "pika": {
            "level": "ERROR"
        },
        "tornado.access": {
            "level": "INFO"
        }, 
        "tornado.application": {
            "level": "INFO"
        }, 
        "tornado.general": {
            "level": "INFO"
        }
    }, 
    "root": {
        "handlers": [
            "brew-view"
        ], 
        "level": "INFO"
    }, 
    "version": 1
}